package com.easytop.psm.model;

public class Retailer {

	private int r_id;
	private String r_name;
	private String r_area;
	private String r_principal;
	private String r_phone;
	private String r_Id_number;
	private String r_site;
	private String r_postcode;
	
	public Retailer() {
		super();
	}
	public Retailer(String r_name, String r_area, String r_principal, String r_phone, String r_Id_number,
			String r_site, String r_postcode) {
		super();
		this.r_name = r_name;
		this.r_area = r_area;
		this.r_principal = r_principal;
		this.r_phone = r_phone;
		this.r_Id_number = r_Id_number;
		this.r_site = r_site;
		this.r_postcode = r_postcode;
	}
	public int getR_id() {
		return r_id;
	}
	public void setR_id(int r_id) {
		this.r_id = r_id;
	}
	public String getR_name() {
		return r_name;
	}
	public void setR_name(String r_name) {
		this.r_name = r_name;
	}
	public String getR_area() {
		return r_area;
	}
	public void setR_area(String r_area) {
		this.r_area = r_area;
	}
	public String getR_principal() {
		return r_principal;
	}
	public void setR_principal(String r_principal) {
		this.r_principal = r_principal;
	}
	public String getR_phone() {
		return r_phone;
	}
	public void setR_phone(String r_phone) {
		this.r_phone = r_phone;
	}
	public String getR_Id_number() {
		return r_Id_number;
	}
	public void setR_Id_number(String r_Id_number) {
		this.r_Id_number = r_Id_number;
	}
	public String getR_site() {
		return r_site;
	}
	public void setR_site(String r_site) {
		this.r_site = r_site;
	}
	public String getR_postcode() {
		return r_postcode;
	}
	public void setR_postcode(String r_postcode) {
		this.r_postcode = r_postcode;
	}
	@Override
	public String toString() {
		return "User [r_id=" + r_id + ", r_name=" + r_name + ", r_area=" + r_area + ", r_principal=" + r_principal
				+ ", r_phone=" + r_phone + ", r_Id_number=" + r_Id_number + ", r_site=" + r_site + ", r_postcode="
				+ r_postcode + "]";
	}
	
	
}
